        <maven.compiler.target>19</maven.compiler.target>
        <exec.mainClass>com.mycompany.demo1.Demo1</exec.mainClass>
        <flatlafVersion>3.3</flatlafVersion>
    </properties>

    <dependencies>
        <dependency>
            <groupId>org.postgresql</groupId>
